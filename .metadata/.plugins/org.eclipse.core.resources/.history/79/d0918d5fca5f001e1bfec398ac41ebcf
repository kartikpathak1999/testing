package DemoProject;
import org.testng.annotations.Test;

public class GroupDemo {

		@Test(groups = {"Mathamatics"})
		
		public void Sum()
		{
			System.out.println("sum belongs to math group");
		}
		
		@Test(groups = {"Group A", "Group B"})
		public void TestB()
		{
			System.out.println("K belong to Group A and Group B");
		}
		
		@Test(groups = {"Mathamatics"})
		public void Divide()
		{
			System.out.println("Divide belongs to maths group");
			
		}
		
		
		

		@Test(groups = {"Group A"})
		
		public void Testsum()
		{
			System.out.println("sum belong to group A");
		}
	}


